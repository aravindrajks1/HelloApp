<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.aravind.dialerapp.GoProFragment">

    <!-- TODO: Update blank fragment layout -->


    <ImageView
        android:id="@+id/img_lck"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="43dp"
        android:tint="@color/colorPrimary"
        app:srcCompat="@android:drawable/ic_lock_lock" />

    <TextView
        android:id="@+id/tv_upgradePremium"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/img_lck"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="75dp"
        android:text="Upgrade To Professional"
        android:textSize="20dp" />

    <Button
        android:id="@+id/btn_buy_pro"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="2dp"
        android:layout_marginRight="2dp"
        android:background="#86e271"
        android:text="Buy Pro" />

    <ImageView
        android:id="@+id/img_close"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentTop="true"
        app:srcCompat="@android:drawable/btn_dialog" />
</RelativeLayout>
